{
  "name": "server2",
  "version": "1.0.0",
  "packageManager": "pnpm@8.15.6",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "dev:https": "ENABLE_DEV_HTTPS=true pnpm exec nodemon src/server.js",
    "start": "node src/server.js",
    "start:https": "ENABLE_DEV_HTTPS=true node src/server.js",
    "build": "echo \"(skip if JS-only)\"",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:verbose": "cross-env NODE_ENV=test jest --verbose",
    "test:silent": "cross-env NODE_ENV=test jest --silent",
    "migrate": "node src/scripts/runMigration.js",
    "migrate:security": "node src/scripts/runMigration.js",
    "generate-dev-certs": "node src/scripts/generateDevCerts.js",
    "https-status": "node -e \"import('./src/config/httpsConfig.js').then(m => console.log(JSON.stringify(m.checkHTTPSStatus(), null, 2)))\""
  },
  "engines": {
    "node": ">=20.16.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "@sentry/integrations": "^7.114.0",
    "@sentry/node": "^10.15.0",
    "@sentry/profiling-node": "^10.15.0",
    "@sentry/tracing": "^7.120.4",
    "@supabase/storage-js": "^2.7.3",
    "@supabase/supabase-js": "^2.50.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "esbuild-loader": "^4.3.0",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.1.0",
    "express-slow-down": "^3.0.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "openai": "^5.23.0",
    "sharp": "^0.33.4",
    "supabase": "^2.26.9",
    "winston": "^3.17.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@types/jest": "^30.0.0",
    "babel-jest": "^30.1.2",
    "cross-env": "^10.0.0",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4"
  }
}
