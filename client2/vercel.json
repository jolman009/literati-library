{
  "buildCommand": "cd client2 && pnpm install --frozen-lockfile && pnpm run build",
  "installCommand": "cd client2 && pnpm install --frozen-lockfile",
  "outputDirectory": "client2/dist",
  "framework": "vite",
  "builds": [
    { "src": "client2/package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "client2/dist/index.html" }
  ]
}
